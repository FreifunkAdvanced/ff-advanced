config wifi-device  wifi0
	option type     atheros
	option channel	11
	option country  276
	option txpower  20

config wifi-iface
	option device	wifi0
	option mode		adhoc
	option ssid		batbone.freifunk-wuppertal.net
	option bssid	02:CA:FF:EE:E1:BA
	option hidden	1
	option encryption	none

config wifi-iface
	option device	wifi0
	option mode		ap
	option network	mesh
	option ssid		www.freifunk-wuppertal.net
	option encryption	none

