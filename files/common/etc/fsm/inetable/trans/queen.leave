#!/bin/sh -e
. ../common_queen.sh

# do not announce gateway anymore
batctl gw client

# release IP in p2ptable (if we still own it)
if we_own_our_ip; then
    p2ptbl update $gwiptbl $oct3 free br-mesh
fi

# disable DHCP
uci delete dhcp.mesh
/etc/init.d/dnsmasq restart

