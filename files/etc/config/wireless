config wifi-device  wifi0
        option type     atheros
        option channel  5

config wifi-iface
        option device   wifi0
        option mode     adhoc
        option ssid     batman.jena.freifunk.net
        option bssid    52:CA:FF:EE:BA:BE
        option hidden   1

config wifi-iface
        option device   wifi0
        option mode     ap
        option ssid     test.freifunk-jena.de

