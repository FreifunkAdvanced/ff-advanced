config wifi-device  wifi0
        option type     atheros
        option channel  5

config wifi-iface
        option device   wifi0
        option mode     adhoc
        option ssid     batman.jena.freifunk.net
        option bssid    03:CA:FF:EE:BA:BE

config wifi-iface
        option device   wifi0
        option mode     ap
        option ssid     test.freifunk-jena.de
        option bssid    04:CA:FF:EE:BA:BE

